{
  "name": "spring-data-starter",
  "env": {
    "SERVICE_ACCOUNT": {
      "description": "Your Astra service account credentials",
      "required": true
    },
    "ASTRA_DB_PASSWORD": {
      "description": "Your Astra database password",
      "required": true
    },
    "ASTRA_DB_KEYSPACE": {
      "description": "Your Astra database keyspace",
      "required": true
    }
  },
  "hooks": {
    "prebuild": {
      "commands": [
        ". ./setup.sh"
      ]
    },
  }
}
